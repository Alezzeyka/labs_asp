@model HomeViewModel
<body>
<div class="row mt-5 mb-2">
    <h1>Вся посуда</h1>
    <form class="d-flex" asp-controller="Dish" asp-action="ListName" method="post" >
          <input class="form-control me-2" type="text" placeholder="Поиск" aria-label="Search" name="name">
          <button class="btn btn-outline-success" type="submit">Найти</button>
    </form>
     @{
         foreach (Dish dish in Model.dishes){
         @Html.Partial("AllDish", dish)
         }
     }
</div>
</body>

